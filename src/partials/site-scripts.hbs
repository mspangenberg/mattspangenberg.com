<script type="text/javascript">
	$(document).ready(function() {

		$('#fullpage').fullpage({
			'verticalCentered': false,
			'css3': true,
			'scrollOverflow': true,
			'menu': '#menu',
			'sectionsColor': ['#333333', '#666666', '#212121', '#727272', '#ff0000'],
			'anchors': ['about', 'experience', 'skills', 'work'],
			'slidesNavigation': true,

			'afterRender': function(){
				$('#angle-down').addClass('animated bounce');
			},

			'afterLoad': function(anchorLink, index){
				if(index == 1){
					$('#angle-down').addClass('animated bounce');
				}
				else if(index == 3){
					$('#iphone2').addClass('active');
				}
			},

			'onLeave': function(index, nextIndex, direction){
				$('#menu').toggleClass('menu-fixed', (index == 1 && direction == 'down' ) || (index == 2 && direction == 'down' ) || (index == 3 && direction == 'down' ) || (index == 4 && direction == 'up') || (index == 3 && direction == 'up'));

				$('#menu').toggleClass('menu-visible', (index == 1 && direction == 'down' ) || (index == 2 && direction == 'down' ) || (index == 3 && direction == 'down' ) || (index == 4 && direction == 'up') || (index == 3 && direction == 'up'));

				$('#nav-background').toggleClass('nav-background-visible', (index == 1 && direction == 'down' ) || (index == 2 && direction == 'down' ) || (index == 3 && direction == 'down' ) || (index == 4 && direction == 'up') || (index == 3 && direction == 'up'));
				$('#angle-down').removeClass('animated bounce');
				$('#iphone2').toggleClass('active', (index == 4 && direction == 'down' ) || (index == 4 && direction == 'up'));

				//	Deplay Example
				//  var delay = setTimeout(function(){
				//  $('#nav-background').removeClass('nav-background-visible');
				//  }, 705)
			}
		});

		$('#section0').addClass('animated zoomIn');

	});
</script>

